#     print("ODD")

dish = input("Enter the food name: ")
indian = ["samosa", "singara"]
chinese = ["soup", "wanthon"]

if dish in indian :
    print("indian")
elif dish in chinese:
    print("chinese")
else:
    print("dont know")    
